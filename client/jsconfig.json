{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@Common/*": ["./src/modules/common/*"],
      "@Public/*": ["src/modules/public/*"],
      "@Private/*": ["src/modules/private/*"],
      "@Home": ["src/modules/public/home"],
